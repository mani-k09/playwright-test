{
    "default":{
        "formatOptions":{
            "snippetInterface":"async-await"
        },
        "paths":[
            "src/test/features/newCourse.feature"
            
    ],
    "dryRun":false,
    "require":[
        "src/test/steps/*.ts",
        "src/test/hooks/*.ts"
    ],
    "requireModule":[
        "ts-node/register"
    ],
    "format":[
        "progress-bar",
        "html:test-results/cucumber-report.html",
        "json:test-results/cucumber-report.json",
        "rerun:@failedTests.txt"

    ],
    "parallel": 2
    },
    "rerun":{
        "formatOptions":{
            "snippetInterface":"async-await"
        },
    "dryRun":false,
    "require":[
        "src/test/steps/*.ts",
        "src/test/hooks/*.ts"
    ],
    "requireModule":[
        "ts-node/register"
    ],
    "format":[
        "progress-bar",
        "html:test-results/cucumber-report.html",
        "json:test-results/cucumber-report.json",
        "rerun:@failedTests.txt"

    ],
    "parallel": 2
    }
}
